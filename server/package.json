    "@fastify/cors": "^11.0.1",
    "@fastify/swagger": "^9.4.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.57.1",
    "@scalar/fastify-api-reference": "^1.28.12",
    "drizzle-kit": "^0.30.6",
    "drizzle-orm": "^0.41.0",
    "fastify": "^5.2.2",
    "fastify-type-provider-zod": "^4.0.2",
    "pino-pretty": "^13.0.0",
